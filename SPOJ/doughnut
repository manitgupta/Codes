#include <iostream>

using namespace std;

int main ()
{
    int t,c[10],k[10],w[10];
    cin>>t;
    cout<<endl;
    for(int i=0;i<t;i++)
    	{
    		cin>>c[i]>>k[i]>>w[i];
    	}
	for(int j=0;j<t;j++)
		{
			if(c[j]<=(k[j]/w[j]))
				cout<<"yes"<<endl;
			else
				cout<<"no"<<endl;
		}
return 0;
}