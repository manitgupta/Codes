#include <iostream>
#include <cstdlib>

using namespace std;

int main()
{
	int s,count,quad[100001],nilo[100001];
	cin>>s;
	for(int i=0;i<s;i++)
	{
		cin>>quad[i];
	}
	for(int i=0;i<s;i++)
	{
		cin>>nilo[i];
	}
	for(int i=0;i<s;i++)
	{
		for(int j=quad[i];;j++)
		{
			int k=0;
			if(j==nilo[k])
			{
				count++;
				break;
			}
			k++;
		}
	}

	
}